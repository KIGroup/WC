<Method name="IsExists">
<ClassMethod>1</ClassMethod>
<FormalSpec>competitionId:%String,manNumber:%String,womanNumber:%String</FormalSpec>
<ReturnType>%Boolean</ReturnType>
<Implementation><![CDATA[
	quit:'##class(Competition).%ExistsId(competitionId) $$$NO
	
	set isSolo = $$$NO
	&sql(SELECT IsSolo INTO :isSolo FROM WC_Data.Competition WHERE ID = :competitionId)
	
	if (isSolo){
		set number = manNumber
		if (number = "") set number = womanNumber
		
		quit ##class(ParticipantSingle).IsExists(competitionId, number)
	}
	else{
		quit ##class(ParticipantCouple).IsExists(competitionId, manNumber, womanNumber)
	}
	
	quit $$$NO
]]></Implementation>
</Method>

